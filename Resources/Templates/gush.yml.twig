# Gush configuration file, any comments will be lost.
meta-header: |
    {{ include('Licenses/' ~ license|lower ~ '.txt.twig', { _block: 'header' })|trim|indent_lines }}
table-pr:
    bug_fix: ['Bug Fix?', 'no']
    new_feature: ['New Feature?', 'no']
    bc_breaks: ['BC Breaks?', 'no']
    deprecations: ['Deprecations?', 'no']
    fixed_tickets: ['Fixed Tickets', '']
    license: ['License', 'MIT']
pr_type:
    - feature
    - bug
    - refactor
    - style
    - documentation
    - security
